     <RmGLESShader NAME="Fragment Program" API="OpenGL ES" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
#ifdef GL_FRAGMENT_PRECISION_HIGH
   // Default precision
   precision highp float;
#else
   precision mediump float;
#endif

uniform sampler2D tex;
uniform float particleShape;

varying vec2  v_tex;
varying float v_alpha;

void main(void)
{ 
   gl_FragColor = texture2D(tex, v_tex) * v_alpha;
}]]></RmShaderCode></RmGLESShader>
     <RmGLESShader NAME="Vertex Program" API="OpenGL ES" PIXEL_SHADER="FALSE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform float time_0_X;
uniform mat4  view_inverse_matrix;
uniform mat4  view_proj_matrix;

uniform vec4 view_position;
uniform mat4 view_matrix;

uniform float particleSystemHeight;
uniform float particleSpeed;
uniform float particleSpread;
uniform float particleSystemShape;
uniform float particleShape;
uniform float particleSize;
uniform vec3 windDirection;
uniform vec3 particleSystemPosition;

attribute vec4 rm_Vertex;

varying vec2  v_tex;
varying float v_alpha;

void main(void)
{
   float t = fract(rm_Vertex.z + particleSpeed * time_0_X);
   float s = pow(t, particleSystemShape);

   vec3 pos;
   pos.x = sin(62.0 * rm_Vertex.z);
   pos.z = cos(163.0 * rm_Vertex.z);

   pos.xz = normalize(pos.xz) * particleSpread * cos(1231.0 * rm_Vertex.z) * s;
   
   pos.y = particleSystemHeight * t;
   
   pos.xyz += pow(windDirection, vec3(t));
   
   vec3 d = normalize(view_matrix[3].xyz - pos);
   vec3 r = normalize(cross(d, vec3(0, 1, 0)));
   vec3 u = normalize(cross(d, r));

   pos += particleSystemPosition + transpose(mat3(view)) * (particleSize * rm_Vertex.xyz);

   
   gl_Position = view_proj_matrix * vec4(pos,1.0);
   
   v_tex = rm_Vertex.xy / 2.0 + vec2(0.5);
   v_alpha = pow(1.0 - t, 2.0);
}]]></RmShaderCode></RmGLESShader>
